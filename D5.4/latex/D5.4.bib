	Date-Modified = {2016-05-20 12:17:22 +0000},
	Month = {May},
	Organization = {IEEE},
	Pages = {5309--5316},
	Read = {1},
	Title = {Soft wearable motion sensing suit for lower limb biomechanics measurements},
	Year = {2013}}

